{
  "name": "boilerplate-loopback",
  "version": "1.0.0",
  "main": "src/server.js",
  "engines": {
    "node": ">=4"
  },
  "description": "A boilerplate for a RESTful API (api only) using LoopBack",
  "repository": {
    "type": "git",
    "url": "https://github.com/tientran0019/boilerplate-loopback"
  },
  "author": "Trần Đức Tiến <tientran0019@gmail.com>",
  "keywords": [
    "api",
    "boilerplate",
    "loopback",
    "webpack",
    "nodemon",
    "babel"
  ],
  "license": "MIT",
  "private": false,
  "scripts": {
    "lint": "eslint .",
    "posttest": "npm run lint && nsp check",
    "start": "npm run dev",
    "dev": "nodemon . --exec babel-node",
    "prod": "NODE_ENV=production babel-node .",
    "prod:pm2": "NODE_ENV=production pm2 start src/server.js --name loopback-boilerplate --interpreter babel-node"
  },
  "dependencies": {
    "async": "^2.6.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "connect-mongo": "^2.0.3",
    "cors": "^2.8.5",
    "cron": "^1.6.0",
    "dotenv": "^6.2.0",
    "fb": "^2.0.0",
    "google-auth-library": "0.12.0",
    "helmet": "^3.15.0",
    "lodash": "^4.17.11",
    "loopback": "^3.25.0",
    "loopback-boot": "^2.27.1",
    "loopback-component-explorer": "^6.3.1",
    "loopback-component-storage": "^3.5.0",
    "loopback-connector-mongodb": "^3.9.2",
    "loopback-counts-mixin": "^1.0.1",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.9",
    "pretty-error": "^2.1.1",
    "serve-favicon": "^2.5.0",
    "slugify": "^1.3.4",
    "strong-error-handler": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-module-resolver": "^3.1.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-loopback": "^13.0.0",
    "eslint-import-resolver-babel-module": "^5.0.1",
    "eslint-plugin-import": "^2.16.0",
    "nsp": "^3.2.1"
  }
}
